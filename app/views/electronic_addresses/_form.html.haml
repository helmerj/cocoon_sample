= simple_form_for @electronic_address, :html => { :class => 'form-horizontal' } do |f|
  = f.error_notification

  %legend
    = controller.action_name.capitalize
    =t('electronic_address.model_name')


  .form-inputs
    = f.input :electronic_address_string, label: t('electronic_address.electronic_address_string')

  .form-actions
    = f.submit t('actions.save'), class: 'btn btn-primary btn-mini'
    = link_to t('actions.back'),electronic_addresses_path, class: 'btn btn-mini'
